{
  "name": "foxess-lib",
  "displayName": "FoxESS Lib",
  "version": "0.1.2",
  "description": "Wrapper for the FoxESS OpenAPI specification",
  "license": "GPL-3.0",
  "keywords": [
    "foxess",
    "foxesscloud",
    "energizer",
    "solar",
    "energizersolar",
    "openapi"
  ],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git://github.com/teh-hippo/foxess-lib.git"
  },
  "bugs": {
    "url": "http://github.com/teh-hippo/foxess-lib/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/teh-hippo"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest tests",
    "lint": "eslint --max-warnings=0 src/**/*.ts",
    "lintAndFix": "eslint --fix --max-warnings=0 src/**/*.ts",
    "prepublish": "tsc",
    "prepublishOnly": "npm run lint && npm test",
    "updateOpenAPI": "tsx tools/generator.ts && npx openapi-typescript foxess-api.json -o src/v1.ts && eslint --fix ./src/v1.ts"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.6.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "node-html-parser": "^6.1.12",
    "openapi-typescript": "^6.7.4",
    "ts-jest": "^29.1.2",
    "ts-log": "^2.2.5",
    "ts-patch": "^3.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.0.2"
  },
  "dependencies": {
    "openapi-fetch": "^0.9.1",
    "openapi-types": "^12.1.3",
    "ts-md5": "^1.3.1"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ]
}
