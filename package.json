{
  "name": "foxess-lib",
  "displayName": "FoxESS Lib",
  "version": "0.7.2",
  "description": "Wrapper for the FoxESS OpenAPI specification",
  "license": "GPL-3.0",
  "keywords": [
    "foxess",
    "foxesscloud",
    "energizer",
    "solar",
    "energizersolar",
    "openapi"
  ],
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/teh-hippo/foxess-lib.git"
  },
  "bugs": {
    "url": "http://github.com/teh-hippo/foxess-lib/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/teh-hippo"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.js",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "require": {
        "types": "./lib/index.d.cts",
        "default": "./lib/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "jest tests",
    "lint": "eslint src tools",
    "lintAndFix": "eslint --fix src tools",
    "updateOpenAPI": "pnpm exec tsx tools/generator.ts && pnpm exec openapi-typescript foxess-api.json -o src/v1.ts && eslint --fix ./src/v1.ts",
    "release": "release-it"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@eslint/js": "^9.39.1",
    "@stylistic/eslint-plugin": "^5.6.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "jest": "^30.2.0",
    "node-html-parser": "^7.0.1",
    "openapi-typescript": "^7.10.1",
    "ts-jest": "^29.4.5",
    "ts-log": "^2.2.7",
    "ts-patch": "^3.3.0",
    "tsup": "^8.5.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0"
  },
  "dependencies": {
    "openapi-fetch": "^0.15.0",
    "openapi-types": "^12.1.3",
    "ts-md5": "^2.0.1"
  },
  "files": [
    "lib",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "packageManager": "pnpm@10.22.0+sha512.bf049efe995b28f527fd2b41ae0474ce29186f7edcb3bf545087bd61fbbebb2bf75362d1307fda09c2d288e1e499787ac12d4fcb617a974718a6051f2eee741c"
}
